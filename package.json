{
  "name": "rest-resource",
  "version": "0.4.5",
  "description": "Simplify your REST API resources, supports automatic caching",
  "main": "./dist/index.js",
  "module": "./es/index.js",
  "types": "types",
  "scripts": {
    "test": "mocha -r ts-node/register ./tests/**/*.ts",
    "build": "tsc --target es5 --module commonjs --outDir ./dist",
    "dev": "tsc --watch",
    "prettier": "./node_modules/prettier/bin-prettier.js --write ./src/**/*.ts ./src/*.ts",
    "docs": "documentation build dist/index.js -f md",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baseprime/rest-resource.git"
  },
  "keywords": [
    "rest",
    "api",
    "model",
    "cache",
    "caching",
    "express",
    "restify",
    "vue"
  ],
  "author": "Greg Sabia Tucker <greg@narrowlabs.com> (http://basepri.me)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/baseprime/rest-resource/issues"
  },
  "homepage": "https://github.com/baseprime/rest-resource#readme",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.19",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "prettier": "^1.16.2",
    "ts-node": "^8.0.2",
    "tslib": "^1.9.3",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "assert": "^1.4.1",
    "axios": "^0.18.0",
    "lodash": "^4.17.11"
  }
}
